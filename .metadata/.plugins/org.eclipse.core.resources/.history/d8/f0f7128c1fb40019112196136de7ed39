public class Hello{

	public static void main(String argv[]){
		
		// Get the argument then output it
		argc = argv.length;
		
		if(argv.length >= 0){
			
			for(int i = 0 ; i < argv.length ; i++){
							
				System.out.print(argv[i].toString());
			}
		}
		
		else{
			
			System.out.print("Hello World!!\n");
		}
	}
}
